{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- tsx watch --ignore \"./vite.config.ts.timestamp-*.mjs\" server/dev.ts",
    "build": "vite build && npx esbuild server/index.ts --platform=node --bundle --outfile=dist/index.js --format=esm --define:process.env.NODE_ENV='\"production\"' --packages=external",
    "start": "NODE_ENV=production node dist/index.js",
    "build:vercel": "vite build",
    "check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force"
  },
  "vitest": {
    "include": [
      "**/*.{test,spec}.{js,mjs,cjs,ts,mts,cts,jsx,tsx}"
    ],
    "environment": "node",
    "coverage": {
      "reporter": [
        "text",
        "json",
        "html"
      ],
      "exclude": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/node_modules/**",
        "**/dist/**",
        "**/coverage/**"
      ]
    }
  },
